<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="org.andriy.spring.entities.I18NMessage" table="I18NMessage">
	    <id name="id" type="int" column="id">
	    	<generator class="native"/>
		</id>
		<property name="messageCode" column="message_code" type="string"/>
		<property name="messageRU" column="message_RU" type="string"/>
		<property name="messagePT" column="message_PT" type="string"/>
		<property name="messageEN" column="message_EN" type="string"/>
	</class>
</hibernate-mapping>